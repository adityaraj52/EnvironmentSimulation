<?xml-stylesheet type="text/xsl" href="query.xsl"?>

<query xmlns:vc="http://www.w3.org/2007/XMLSchema-versioning"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="query.xsd">

    <filter>
        <item>
            <key>highway</key>
            <r_operator>equals</r_operator>
            <value>value0</value>
        </item>
    </filter>
    <filter>
        <item>
            <key>highway</key>
            <r_operator>equals</r_operator>
            <value>value1</value>
        </item>
    </filter>
    <polynomial>
        <circle>
            <centre latitude="0" longitude="0"/>
            <radius>50</radius>
        </circle>
    </polynomial>
</query>
